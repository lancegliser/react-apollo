{
  "name": "react-apollo",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "vite --host 127.0.0.1 --port 3000",
    "build": "tsc --noEmit && vite build",
    "test": "react-scripts test",
    "graphql-codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx src/ --color",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "serve": "vite preview --host 127.0.0.1 --port 3000"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.14.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-react-app": "^7.0.1",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "react-error-overlay": "^6.0.11",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,graphql}": [
      "prettier --write"
    ]
  }
}
